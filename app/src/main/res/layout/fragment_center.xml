            android:background="@color/whiteBgColor">

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/imageView38"
                android:layout_width="@dimen/px_48"
                android:layout_height="@dimen/px_48"
                android:layout_margin="@dimen/px_17"
                android:background="@drawable/icon_mycollect" />

            <android.support.v7.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px_text_34"
                android:textColor="@color/subordinationTitleTextColor"
                android:layout_marginLeft="@dimen/px_15"
                android:layout_marginRight="@dimen/px_15"
                android:layout_marginBottom="@dimen/px_15"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView38"
                android:text="我的收藏" />

            <android.support.v7.widget.AppCompatImageView
