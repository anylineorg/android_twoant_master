<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tool="http://schemas.android.com/tools"
    android:background="@color/backgroundColor">
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_marginBottom="@dimen/px_40"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:paddingTop="@dimen/px_10"
            android:paddingBottom="@dimen/px_10"
            android:id="@+id/rl_xinxi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/whiteBgColor">

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/tv_name"
                android:layout_toRightOf="@+id/iv_photo_center"
                android:marqueeRepeatLimit="marquee_forever"
                android:layout_marginTop="@dimen/px_15"
                android:scrollHorizontally="true"
                android:focusableInTouchMode="true"
                android:focusable="true"
                android:ellipsize="end"
                android:singleLine="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px_text_34"
                android:textColor="@color/subordinationTitleTextColor"
                android:layout_marginLeft="@dimen/px_18"
                android:layout_marginRight="@dimen/px_20"
                android:layout_marginBottom="@dimen/px_10"
                tool:text="鱼丸" />

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px_text_28"
                android:textColor="@color/light_white"
                android:layout_marginLeft="@dimen/px_18"
                android:layout_marginRight="@dimen/px_15"
                android:layout_marginBottom="@dimen/px_15"
                android:layout_toRightOf="@+id/iv_photo_center"
                android:layout_below="@+id/tv_name"
                android:text="账号:" />

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/tv_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px_text_28"
                android:layout_marginBottom="@dimen/px_15"
                android:layout_toRightOf="@+id/number"
                android:textColor="@color/light_white"
                android:layout_below="@+id/tv_name"
                tool:text="123456789" />

            <android.support.v7.widget.AppCompatImageView
                android:padding="@dimen/px_10"
                android:id="@+id/iv_editdata_center"
                android:layout_width="@dimen/px_60"
                android:layout_height="@dimen/px_60"
                android:layout_marginRight="@dimen/px_17"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/qianbi_bianji"
                android:gravity="center"
                android:orientation="vertical"
                />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/iv_photo_center"
                android:layout_width="@dimen/px_93"
                android:layout_marginLeft="13dp"
                android:layout_height="@dimen/px_93"
                tool:src="@drawable/shezhitouxiang"
                android:layout_alignBottom="@+id/number"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="13dp" />
        </RelativeLayout>

        <LinearLayout
            android:padding="@dimen/px_16"
            android:layout_marginTop="@dimen/px_2"
            android:background="@color/whiteBgColor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px_text_34"
                android:text="个性签名"
                android:textColor="@color/subordinationTitleTextColor" />

            <TextView
