                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px_text_30"
                android:textColor="@color/subordinationContentTextColor"
                tool:text="个性签名"/>
        </LinearLayout>

        <RelativeLayout
